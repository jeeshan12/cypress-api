{
  "name": "cypress-api-automation",
  "version": "1.0.0",
  "description": "API Automation with Cypress",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write 'cypress/'",
    "pretest": "node reportsetup.js",
    "test": "cypress run",
    "generatereport": "npx mochawesome-merge 'cypress/results/*.json' > mochawesome.json",
    "postgeneratereport": "npx marge ./cypress/results/mochawesome.json"
    "test:ci": "cypress run"
  },
  "keywords": [
    "Cypress",
    "API",
    "Rest",
    "API",
    "GraphQL"
  ],
  "author": "Mohd Jeeshan",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^10.1.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "^2.7.1",
    "randomstring": "^1.2.2",
    "uuid": "^8.3.2"
  }
}
